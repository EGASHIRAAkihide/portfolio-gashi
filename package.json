{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "prepare": "husky install",
    "style": "npx prettier --write app/**/*.{ts,tsx} components/**/*.tsx && yarn lint"
  },
  "dependencies": {
    "@types/node": "18.14.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.34.0",
    "eslint-config-next": "13.2.1",
    "next": "^13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7"
  },
  "lint-staged": {
    "appDir/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write appDir/**/*.{ts,tsx}",
      "yarn lint"
    ],
    "components/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write components/**/*.{ts,tsx}"
    ]
  }
}
